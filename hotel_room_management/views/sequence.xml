<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record id="seq_hotel_accommodation" model="ir.sequence">
            <field name="name">hotel accommodation</field>
            <field name="code">hotel.accommodation.sequence</field>
            <field name="active">TRUE</field>
            <field name="prefix">HM/ROOM/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>

        </record>
         <record id="seq_hotel_order" model="ir.sequence">
            <field name="name">hotel order</field>
            <field name="code">hotel.order.sequence</field>
            <field name="active">TRUE</field>
            <field name="prefix">HM/ORDER/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>

        </record>


</odoo>